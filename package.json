{
  "name": "MapMakerApi",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "pact": "MOCHA_FILE=$CIRCLE_TEST_REPORTS/junit/pact-results.xml ./node_modules/.bin/mocha pacts/mapmaker.spec.js --reporter mocha-junit-reporter",
    "pact-prod": "MOCHA_FILE=$CIRCLE_TEST_REPORTS/junit/pact-prod-results.xml env TAG=prod ./node_modules/.bin/mocha pacts/mapmaker.spec.js  --reporter mocha-junit-reporter",
    "mocha": "MOCHA_FILE=$CIRCLE_TEST_REPORTS/junit/test-results.xml mocha ./public/javascripts/*.spec.js --reporter mocha-junit-reporter",
    "fitnesse": "cd fitnesse && ./runtest.sh"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "mocha": "^3.4.2",
    "mocha-junit-reporter": "^1.13.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "pact": "^2.5.0",
    "pact-js-mocha": "^1.0.2-alpha"
  }
}
